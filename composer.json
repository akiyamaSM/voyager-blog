{
    "name": "clevyr/voyager-blog",
    "description": "Voyager blog package using Laraberg",
    "type": "library",
    "authors": [
        {
            "name": "Anthony Protano",
            "email": "anthony@clevyr.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:clevyr/laraberg.git"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "tcg/voyager": "^1.3",
        "clevyr/laraberg": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Clevyr\\VoyagerBlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clevyr\\VoyagerBlog\\VoyagerBlogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
